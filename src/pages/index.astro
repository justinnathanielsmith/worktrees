---
import BaseLayout from '../layouts/BaseLayout.astro';
import Aurora from '../components/Aurora.astro';
import Navigation from '../components/Navigation.astro';
import Header from '../components/Header.astro';
import QuickInstall from '../components/QuickInstall.astro';
import FeatureGrid from '../components/FeatureGrid.astro';
import Protocol from '../components/Protocol.astro';
import Efficiency from '../components/Efficiency.astro';
import Commands from '../components/Commands.astro';
import Footer from '../components/Footer.astro';
---

<BaseLayout title="Bare Hub | High-Performance Git Architecture">
